        <receiver bindType="ESME">
            <host>localhost</host>
            <port>34003</port>
            <systemId>ArchTel</systemId>
            <password>MaRchT3l</password>
            <bindOption>receiver</bindOption>
            <threads>500</threads>
            <concatenation>UDHI</concatenation>
            <encoding>Cp1252</encoding>
        </receiver>
    </connection>
    <connection id="420">
        <name>HKCSL - Archive</name>
        <automatic_dr>DELIVRD</automatic_dr>
        <receiver bindType="ESME">
            <host>localhost</host>
            <port>34004</port>
            <systemId>TeleMsg</systemId>
