    <connection id="400">
        <name>Telus - Archive</name>
        <automatic_dr>DELIVRD</automatic_dr>
        <receiver bindType="ESME">
            <host>localhost</host>
            <port>34002</port>
            <systemId>TlmsgArch1</systemId>
            <password>password</password>
            <bindOption>receiver</bindOption>
            <threads>500</threads>
            <concatenation>UDHI</concatenation>
        </receiver>
    </connection>

    <connection id="410">
        <name>Rogers - Archive</name>
